%form{ :action => "/api/telephony_account/#{@ta.id}/keyword", :method => 'POST' }
  %input{ :type => 'text', :name => 'word' }
  %textarea{ :name => 'response' }
  %input{ :type => 'submit', :class => 'btn btn-default', :value => 'add keyword'}

%ul
  - @telephony_account_keywords.each do |word|
    %li
      #{word.word}
